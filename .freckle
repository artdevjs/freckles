- python-dev:
   python_dev_python_version: 2.7
   python_dev_setup_type: "pip"
   python_dev_requirements_files:
      - requirements_dev.txt
   install:
      - pycrypto-related:
          pkgs:
            debian:
              - libssl-dev
              - libffi-dev
              - libsqlite3-dev
            conda:
              - cryptography
              - pycrypto
            other: omit

- vagrant-dev:
    __auto_run__: false
    vagrant_dev_providers:
      - virtualbox

# not implemented yet
- docker-dev:
    freckle_profile_active: false
