vars:   
   freckle_profiles:
      - python-dev
   pkg_mgr: conda
   conda_environment: freckles-dev
   packages:
      - base_dependencies:
          pkg_mgr: auto
          pkgs:
            debian:
              - build-essential
      - dependencies:
          pkgs:
            debian:
              - python-dev
              - python-virtualenv
              - libssl-dev
              - libffi-dev
              - libsqlite3-dev
            conda:
              - python
              - pip
              - cryptography
              - pycrypto
